{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Settings": {
    "BlipBotSettings": {
      "Identifier": "botIdentifier",
      "AccessKey": "#{blipapitemplate-BlipAccessKey}#",
      "Authorization": "#{blipapitemplate-BlipAuthorizationKey}#"
    },
    "CheckAuthorizationKey": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://www.google.com",
          "apiKey": "#{blipapitemplate-SeqApiKey}#"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
