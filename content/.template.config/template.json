{
  "$schema": "http://json.schemastore.org/template",
  "author": "Lucas Mundim",
  "classifications": [ "API", "ASP.NET Core", "BLiP" ],
  "identity": "Blip.Api.Template.K8s",
  "name": "BLiP API Template - Kubernetes",
  "shortName": "blip-api-k8s",
  "tags":{
    "language": "C#" 
  },
  "symbols": {
    "Services": {
      "type": "parameter",
      "datatype": "string",
      "description": "Defines the name of the Services project",
      "FileRename": "Blip.Api.Template.Services",
      "defaultValue": "Blip.Api.Template.Services"
    },
    "aks":{
      "type": "parameter",
      "datatype": "string",
      "replaces": "blipapitemplate",
      "isRequired": true,
      "description": "Chart name to be used. Must be in lower-kebab-case",
      "fileRename": "blipapitemplate"
    },
    "sonarqube-guid-ui":{
      "type": "generated",
      "generator": "guid",
      "replaces": "sonarqube-guid-ui",
      "parameters": {
        "format":"D"
      }
    },
    "sonarqube-guid-facades":{
      "type": "generated",
      "generator": "guid",
      "replaces": "sonarqube-guid-facade",
      "parameters": {
        "format":"D"
      }
    },
    "sonarqube-guid-models":{
      "type": "generated",
      "generator": "guid",
      "replaces": "sonarqube-guid-model",
      "parameters": {
        "format":"D"
      }
    },
    "sonarqube-guid-services":{
      "type": "generated",
      "generator": "guid",
      "replaces": "sonarqube-guid-services",
      "parameters": {
        "format":"D"
      }
    },
    "sonarqube-guid-tests":{
      "type": "generated",
      "generator": "guid",
      "replaces": "sonarqube-guid-tests",
      "parameters": {
        "format":"D"
      }
    }
  },
  "sourceName": "Blip.Api.Template",
  "preferNameDirectory": true
}